{
  "minimum_chrome_version": "21",
  "manifest_version": 2,
  "offline_enabled": true,
  "name": "Gforth",
  "description": "Gforth",
  "version": "0.7.0.3",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
